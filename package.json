{
  "name": "smart-leader-real-estate",
  "version": "1.0.0",
  "description": "Full-stack real estate project - Smart Leader",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "cd frontend && npm start",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "clean": "node scripts/cleanup.js",
    "analyze": "node scripts/analyze-size.js",
    "clean:all": "npm run clean && rm -rf node_modules frontend/node_modules backend/node_modules && npm run install:all",
    "migrate:firebase": "node scripts/migrate-to-firebase.js",
    "migrate:mock": "node scripts/migrate-mock-to-firebase.js",
    "update:backend": "node scripts/update-backend-to-firebase.js",
    "docker:build": "docker build -f Dockerfile.optimized -t smart-leader .",
    "docker:run": "docker run -p 3000:3000 -p 5000:5000 smart-leader"
  },
  "keywords": ["real-estate", "nextjs", "express", "mongodb"],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "mongoose": "^8.0.0",
    "firebase": "^10.7.0",
    "dotenv": "^16.3.1"
  }
}
